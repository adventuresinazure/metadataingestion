{
	"name": "AzureMapsToDataLake",
	"properties": {
		"activities": [
			{
				"name": "get-current-weather",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Set variable1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "AzureMaps",
						"type": "DatasetReference",
						"parameters": {
							"api-version": {
								"value": "@pipeline().parameters.api-version",
								"type": "Expression"
							},
							"query": {
								"value": "@pipeline().parameters.query",
								"type": "Expression"
							},
							"subscription-key": {
								"value": "@pipeline().parameters.subscription-key",
								"type": "Expression"
							},
							"path": {
								"value": "@pipeline().parameters.relative-path",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "json_ingestion_input_file",
						"type": "DatasetReference",
						"parameters": {
							"filePath": {
								"value": "@concat('weather/loadDate=', formatDateTime(pipeline().parameters.executionTime, 'yyyy-MM-dd'), '/hour=', formatDateTime(pipeline().parameters.executionTime, 'HH'), '/')",
								"type": "Expression"
							},
							"fileName": {
								"value": "@concat('weather-', formatDateTime(pipeline().parameters.executionTime, 'yyyyMMddHHmm'), '.json')",
								"type": "Expression"
							},
							"containerName": "raw"
						}
					}
				]
			},
			{
				"name": "Set variable1",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "relativePath",
					"value": {
						"value": "@concat(pipeline().parameters.relative-path, 'api\\-version=', pipeline().parameters.api-version, '&query=', pipeline().parameters.query, '&subscription\\-key=', pipeline().parameters.subscription-key)",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"executionTime": {
				"type": "string",
				"defaultValue": "2020-10-05 01:01:00"
			},
			"api-version": {
				"type": "string",
				"defaultValue": "1.0"
			},
			"subscription-key": {
				"type": "string",
				"defaultValue": "8lSTicMGFukVS5LUfCG_8Ivq7q08YlGj5DBUZhC2nec"
			},
			"query": {
				"type": "string",
				"defaultValue": "42.148924384904156\\,-87.99537852892261"
			},
			"relative-path": {
				"type": "string",
				"defaultValue": "weather/currentConditions/json?"
			}
		},
		"variables": {
			"relativePath": {
				"type": "String",
				"defaultValue": "test"
			}
		},
		"annotations": []
	}
}