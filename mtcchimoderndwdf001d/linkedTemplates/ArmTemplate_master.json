					"CloudSales-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceDatabaseName": {
						"value": "[parameters('CloudSales-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceDatabaseName')]"
					},
					"CloudSales-daily-trigger_properties_azure-sql-data-ingestion_parameters_extractionDateTime": {
						"value": "[parameters('CloudSales-daily-trigger_properties_azure-sql-data-ingestion_parameters_extractionDateTime')]"
