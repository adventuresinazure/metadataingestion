		},
		"CloudSales-daily-trigger_properties_azure-sql-data-ingestion_parameters_extractionDateTime": {
			"type": "string",
			"defaultValue": "@trigger().startTime"
		},
		"CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceServerFQDN": {
			"type": "string",
			"defaultValue": "andrmdwopenhacksourcesql001d.database.windows.net"
		},
		"CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceDatabaseName": {
			"type": "string",
			"defaultValue": "CloudStreaming"
		},
		"CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_extractionDateTime": {
			"type": "string",
			"defaultValue": "@trigger().startTime"
		},
		"ssisIntegrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "mtcchioltp001d.database.windows.net"
		},
		"OnPremRentals-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceServerFQDN": {
			"type": "string",
			"defaultValue": "andrmdwopenhacksourcesql001d.database.windows.net"
