					"CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceDatabaseName": {
						"value": "[parameters('CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceDatabaseName')]"
					},
					"CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_extractionDateTime": {
						"value": "[parameters('CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_extractionDateTime')]"
					},
					"OnPremRentals-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceServerFQDN": {
						"value": "[parameters('OnPremRentals-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceServerFQDN')]"
					},
					"OnPremRentals-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceDatabaseName": {
						"value": "[parameters('OnPremRentals-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceDatabaseName')]"
					},
					"OnPremRentals-daily-trigger_properties_azure-sql-data-ingestion_parameters_extractionDateTime": {
						"value": "[parameters('OnPremRentals-daily-trigger_properties_azure-sql-data-ingestion_parameters_extractionDateTime')]"
					}
				}
			},
			"dependsOn": []
		}
