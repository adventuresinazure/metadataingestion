					"CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceServerFQDN": {
						"value": "[parameters('CloudStreaming-daily-trigger_properties_azure-sql-data-ingestion_parameters_sourceServerFQDN')]"
